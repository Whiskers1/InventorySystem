             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:InventorySystem.View"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TabControl>

            <TabItem Header="Lager Liste">
                <local:StockView x:Name = "StockViewControl" Loaded = "StockViewControl_Loaded"/>
            </TabItem>

            <TabItem Header="Users Liste">
                <local:UserView x:Name = "UserViewControl" Loaded = "UserViewControl_Loaded"/>
            </TabItem>

            <TabItem Header="Udlån Liste">
                <!--<local:LoginView x:Name = "LoginViewControl" Loaded = "LoginViewControl_Loaded"/>-->
            </TabItem>

            <TabItem Header="Udlån">
                <!--<local:LoginView x:Name = "LoginViewControl" Loaded = "LoginViewControl_Loaded"/>-->
            </TabItem>

            <TabItem Header="Log">
                <!--<local:LoginView x:Name = "LoginViewControl" Loaded = "LoginViewControl_Loaded"/>-->
            </TabItem>

        </TabControl>

    </Grid>
</UserControl>
